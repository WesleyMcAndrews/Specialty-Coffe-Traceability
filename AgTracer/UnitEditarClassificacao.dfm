  object Usuarios: TADOQuery
    Connection = IWUserSession.ADOConnection1
    CursorType = ctStatic
    DataSource = ClassificacaoSrc
    Parameters = <
      item
        Name = 'IDClassificacao'
        Attributes = [paSigned]
